{
  "name": "elf-ui",
  "version": "0.0.6",
  "description": "Elf, a React based component library for developers; 基于React的桌面组件库",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/didilinkin/elf-ui"
  },
  "authors": {
    "name": "yanxiaodi",
    "email": "didilinkin@vip.qq.com"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/styled-components": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^1.3.1",
    "jsonlint": "^1.6.3",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-docgen-typescript": "^1.12.3",
    "typescript": "^3.4.2",
    "typescript-eslint-parser": "^22.0.0",
    "umi-library": "^1.3.0",
    "umi-test": "^1.5.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "postversion": "git push --follow-tags",
    "doc:dev": "umi-lib doc dev",
    "doc:build": "umi-lib doc build",
    "doc:deploy": "umi-lib doc deploy",
    "dev": "umi-lib build --watch",
    "build": "umi-lib build",
    "pub": "yarn run test && yarn publish",
    "test": "umi-test",
    "lint": "cross-env eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\""
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "npm run lint",
      "prettier --write",
      "git add"
    ],
    ".*{rc, json}": "jsonlint"
  }
}
